    <script type="text/javascript">
        var reportPath = "http://reports.condodeficiencies.com/ReportViewer?";
        //var reportPath = "http://localhost:57245/ReportViewer?";
        var invoiceGuid = '00000000-0000-0000-0000-000000000000';
        var agencyCode;
        //var isAdminUser;

        $(document).ready
            (function () {
                

            });

        function isAdminUser() {
            
        }

        function SetupDialog() {
            $("#dateTimePrintDate").igDatePicker({
                width: "270px",
                dateInputFormat: "date",
                regional: "en-US",
                required: true,
                nullText: "Please select an invoice date...",
                datepickerOptions: {
                    showWeek: false,
                    numberOfMonths: [1, 2]
                }
            });
            $("#dateTimePrintDate").igDatePicker("value", null);

            $("#comboPrintBuildings").on("igcomboselectionchanged", function (evt, ui) {
                var bg = $("#comboPrintBuildings").igCombo("value");
                var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
                var idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            });

            $("#checkIncludeSuspended").igCheckboxEditor({
                inputName: "includeSuspended",
                checked: false
            });

            $("#checkToExcel").igCheckboxEditor({
                inputName: "toExcel",
                checked: false
            });
        }

        function SetupDialogComponents(showDate, showBuildings, showSuspended, showExcel) {
            if (showDate) {
                $('#dateTimePrintDate').show();
                $('#reportDateLabel').show();
            }
            else {
                $('#dateTimePrintDate').hide();
                $('#reportDateLabel').hide();
            }

            if (showBuildings) {
                $('#comboPrintBuildings').show();
                $('#reportBuildingLabel').show();
            }
            else {
                $('#comboPrintBuildings').hide();
                $('#reportBuildingLabel').hide();
            }

            if (showSuspended) {
                $('#reportIncludeSuspendedLabel').show();
                $('#checkIncludeSuspended').show();
            }
            else {
                $('#reportIncludeSuspendedLabel').hide();
                $('#checkIncludeSuspended').hide();
            }

            if (showExcel) {
                $('#reportExportToExcel').show();
                $('#checkToExcel').show();
            }
            else {
                $('#reportExportToExcel').hide();
                $('#checkToExcel').hide();
            }
        }

        function PrintDeficiencyReport(title) {
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 250,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(true, true, false, false);
                    printLoadBuildings();
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintDeficiencyReport();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
            });
        }

        function PrintBuildingSummary(title) {
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 350,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(true, true, true, true);
                    printLoadBuildings();
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintBuildingSummary();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
            });
        }

        function PrintConsolidatedRentDetail(title) {
            checkAdmin();
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 250,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(true, false, false, true);
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintConsolidatedRentDetail();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
            });
        }

        function PrintConsolidatedAgingDetail(title) {
            checkAdmin();
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 250,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(true, false, false, true);
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintConsolidatedAgingDetail();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
            });
        }

        function PrintUnitListing(title) {
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 250,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(false, true, false, false);
                    printLoadBuildings();
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintUnitListing();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
            });
        }

        function PrintInvoice(title) {
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 250,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(true, true, false, false);
                    printLoadBuildings();
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintInvoice();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
