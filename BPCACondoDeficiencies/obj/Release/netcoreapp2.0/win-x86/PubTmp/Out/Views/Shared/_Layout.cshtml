        function PrintDefaultCertificate(title) {
            $("#dialog-alert").dialog({
                title: title,
                resizable: false,
                height: 250,
                width: 450,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: false,
                open: function (event, ui) {
                    SetupDialogComponents(true, true, false, false);
                    printLoadBuildings();
                    SetupDialog();
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    "OK": function () {
                        DoPrintDefaultCertificate();
                        $(this).dialog("close");
                    },
                    "Cancel": function () {
                        $(this).dialog("close");
                    }
                }
            });

        }

        function printLoadBuildings() {
            $.ajax(
                {
                    type: 'GET',
                    url: '/Home/GetBuildingList',
                    cache: false,
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: printInvoiceLoadBuildingsSuccess,
                    error: function (jqXHR, exception) {
                        console.log(jqXHR);
                        console.log(exception);
                    }
                });
        }

        function printInvoiceLoadBuildingsSuccess(data) {
            $("#comboPrintBuildings").igCombo({
                width: "270px",
                textKey: "Name",
                valueKey: "BuildingGuid",
                dataSource: data,
                dataSourceType: "json",
                filteringType: "local",
                autoComplete: true,
                placeHolder: "Select a building...",
                dropDownOrientation: "bottom"
            });

            $("#comboPrintBuildings").on("igcomboselectionchanged", function (evt, ui) {
                var bg = $("#comboPrintBuildings").igCombo("value");
                var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
                var idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            });

            $(function () {
                $("#dateTimePrintDate").igDatePicker({
                    width: "270px",
                    dateInputFormat: "date",
                    regional: "en-US",
                    required: true,
                    nullText: "Please select an invoice date...",
                    datepickerOptions: {
                        showWeek: false,
                        numberOfMonths: [1, 2]
                    }
                });
                $("#dateTimePrintDate").igDatePicker("value", null);
                $("#dateTimePrintDate").on("igdatepickervaluechanged", function (evt, ui) {
                    var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
                    var idt;
                    if (objDate.getDate() !== "1") {
                        idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                        $("#dateTimePrintDate").igDatePicker("value", idt);
                    }
                    else {
                        idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                    }
                });
            });


        }

        function DoPrintDeficiencyReport() {
            var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
            var idt;
            if (objDate.getDate() !== "1") {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                $("#dateTimePrintDate").igDatePicker("value", idt);
            }
            else {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            }

            var bg = $("#comboPrintBuildings").igCombo("value");
            if (bg !== null) {
                var url = reportPath + 'rt=df&bg=' + bg + '&dt=' + idt;
                window.open(url, '_blank');
            }
        }

        function DoPrintBuildingSummary() {
            var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
            var idt;
            if (objDate.getDate() !== "1") {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                $("#dateTimePrintDate").igDatePicker("value", idt);
            }
            else {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            }

            var bg = $("#comboPrintBuildings").igCombo("value");
            var exc = $("#checkToExcel").igCheckboxEditor("option", "checked");
            var shsu = $("#checkIncludeSuspended").igCheckboxEditor("option", "checked");

            console.log(exc);
            if (bg !== null) {
                var url = reportPath + 'rt=bs&bg=' + bg + '&dt=' + idt + '&exc=' + exc + '&shsu=' + shsu;
                window.open(url, '_blank');
            }
        }

        function DoPrintInvoice() {
            var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
            var idt;
            if (objDate.getDate() !== "1") {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                $("#dateTimePrintDate").igDatePicker("value", idt);
            }
            else {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            }

            var bg = $("#comboPrintBuildings").igCombo("value");
            if (bg !== null) {

                $.ajax(
                    {
                        type: 'GET',
                        async: true,
                        url: '/Home/InvoiceExists',
                        data: { buildingGuid: bg, invoiceDate: idt },
                        dataType: "json",
                        success: function (response) {
                            if (response.invoiceGuid !== '00000000-0000-0000-0000-000000000000') {
                                var url = reportPath + 'rt=i&ig=' + response.invoiceGuid;
                                window.open(url, '_blank');
                            }
                        },
                        error: function (jqXHR, exception) {
                            console.log(jqXHR);
                            console.log(exception);
                        }
                    });
            }
        }

        function DoPrintDefaultCertificate() {
            var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
            var idt;
            if (objDate.getDate() !== "1") {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                $("#dateTimePrintDate").igDatePicker("value", idt);
            }
            else {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            }

            var bg = $("#comboPrintBuildings").igCombo("value");
            if (bg !== null) {
                var url = reportPath + 'rt=dc&bg=' + bg + '&dt=' + idt;
                window.open(url, '_blank');
            }
        }

        function DoPrintUnitListing() {
            var bg = $("#comboPrintBuildings").igCombo("value");

            if (bg === null) { return; }
            if (bg === '00000000-0000-0000-0000-000000000000') { return; }

            var url = reportPath + 'rt=ul&bg=' + bg;
            window.open(url, '_blank');
        }

        function DoPrintConsolidatedRentDetail() {
            var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
            var idt;
            if (objDate.getDate() !== "1") {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                $("#dateTimePrintDate").igDatePicker("value", idt);
            }
            else {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            }

            if (idt === null) { return; }
            var exc = $("#checkToExcel").igCheckboxEditor("option", "checked");
            var url = reportPath + 'rt=cdrd&dt=' + idt + '&exc=' + exc;
            window.open(url, '_blank');
        }

        function DoPrintConsolidatedAgingDetail() {
            var objDate = new Date($("#dateTimePrintDate").igDatePicker("value"));
            var idt;
            if (objDate.getDate() !== "1") {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
                $("#dateTimePrintDate").igDatePicker("value", idt);
            }
            else {
                idt = objDate.getMonth() + 1 + "/1/" + objDate.getFullYear();
            }

            if (idt === null) { return; }
            var exc = $("#checkToExcel").igCheckboxEditor("option", "checked");
            var url = reportPath + 'rt=cdad&dt=' + idt + '&exc=' + exc;
            window.open(url, '_blank');
        }

        function checkSession() {
            $.get("/Home/CheckSession", null, function (data) {
                if (data == 0) {
                    window.location.href = 'Login';
                }
            });
        }
        function checkAdmin() {
            $.get("/Home/CheckAdmin", null, function (data) {
                if (data != 'RED' && data != 'AUTH') {
                    window.location.href = 'AccessDenied';
                }
            });
        }

    </script>
